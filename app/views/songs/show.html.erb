<table>
    <thead>
        <td>Title</td>
        <td>Released</td>
        <td>Release Year</td>
        <td>Artist Name</td>
        <td>Genre</td>
    </thead>
    <tbody>
        <%= @song.each do %>
            <tr>
                <td><%= @song.title %></td>
                <td><%= @song.released %></td>
                <td><%= @song.release_year %></td>
                <td><%= @song.artist_name %></td>
                <td><%= @song.genre %></td>
            </tr>
        <% end %>
    </tbody>
</table>
